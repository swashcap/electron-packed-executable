{
  "name": "electron-packed-executable",
  "version": "1.0.0",
  "description": "Test if Electron can fire up packed executables",
  "main": "app/index.js",
  "scripts": {
    "build": "npm run clean && electron-packager ./ --out builds",
    "build:asar": "npm run build -- --asar",
    "clean": "rm -rf builds",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swashcap/electron-packed-executable.git"
  },
  "keywords": [
    "electron",
    "executable"
  ],
  "author": "Cory Reed <swashcap@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swashcap/electron-packed-executable/issues"
  },
  "homepage": "https://github.com/swashcap/electron-packed-executable#readme",
  "devDependencies": {
    "electron": "^1.4.3",
    "electron-packager": "^8.1.0"
  },
  "private": true,
  "dependencies": {
    "electron-debug": "^1.0.1"
  }
}
